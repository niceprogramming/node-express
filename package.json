{
  "name": "node-express",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./**/*.js",
    "lint:fix": "eslint ./**/*.js --fix",
    "start": "node -r @babel/register ./src/app.js",
    "build": "babel src -d build --delete-dir-on-start"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pino": "^5.13.2",
    "pino-pretty": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": ["yarn lint:fix", "yarn lint", "git add"]
  }
}
